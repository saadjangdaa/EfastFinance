
    <input type="hidden" class="form-control" name="AccgroupID" id="AccgroupID" />
    <div class="row">
        <label class="col-md-6">Account Group Name</label>
        <div class="col-md-6">
            <input type="text" class="form-control " id="Accgroupname" name="Accgroupname" />
        </div>
        <hr />
        <br />
        <br />
        <label class="col-md-6">Account Group Alias</label>
        <div class="col-md-6">
            <input type="text" class="form-control " id="Accgroupalias" name="Accgroupalias" />
        </div>
        <br />
        <br />
        <label class="col-md-6">Primary Group</label>

        <div class="col-md-6">
            <select id="primarygroup" class="form-control">
                <option value="0">---Select---</option>
                <option value="1">Yes</option>
                <option value="2">No</option>
            </select>
        </div>
        <br />
        <br />
        <label class="col-md-6">Account Under Group</label>

        <div id="child" style="display:none;" class="col-md-6">
            <input type="text" class="form-control " value="NULL" placeholder="enter account under group" id="AccUnderGroup" name="AccUnderGroup" />
        </div>

        <div id="child2" style="display:none;" class="col-md-6">
            @Html.DropDownList("accundergroup", (IEnumerable<SelectListItem>)ViewBag.accundergroup, "NULL",htmlAttributes: new { @class = "form-control",@Id= "AccUnderGroup2", @Name= "AccUnderGroup2" })
        
        </div>

        <br />
        <br />
    </div>


