                    <div class="col-md-4">

                        @Html.DropDownList("LocationID", (IEnumerable<SelectListItem>)ViewBag.MaterialCentres, "---Select Location---", htmlAttributes: new { @class = " form-control ", @Id = "LocationID" })



                    </div>

                    <lablel class="col-md-2" style="color:black">To </lablel>
                    <div class="col-md-4">
                        @Html.DropDownList("LocationID2", (IEnumerable<SelectListItem>)ViewBag.MaterialCentres, "---Select Location---", htmlAttributes: new { @class = " form-control ", @Id = "LocationID2" })

                    </div>

