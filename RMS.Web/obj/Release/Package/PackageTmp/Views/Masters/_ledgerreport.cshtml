@model  RMS.Web.Models.ViewModels.reportmodel


<div class="modal-dialog ">
    <div class="modal-content report" style="width: @Model.DialogWidth; margin-left: -25%; margin-top: -12%;   ">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title" id="myModalLabel">@Model.Title</h4>
        </div>
        <div class="modal-body">
            <!-- Hello -->
            <iframe src="@Model.Url" style="width: @Model.Width; height: @Model.Height; " marginheight="30px" frameborder="0" scrolling="auto" class="report"></iframe>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->